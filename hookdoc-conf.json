{
  "opts": {
    "destination": "hookdocs",
    "recurse": true,
    "readme": "./README.md"
  },
  "source":
  {"include": [
    "nightingale-companion.php",
    "performance/"
  ],
    "includePattern": ".+\\.(php)?$"
  },
  "plugins": [
    "node_modules/wp-hookdoc/plugin",
    "plugins/markdown"
  ]
}
